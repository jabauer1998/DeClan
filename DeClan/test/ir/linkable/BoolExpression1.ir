SYMBOL SECTION
 ENTRY b CONST INTERNAL GLOBAL a  <INT>
 ENTRY d CONST INTERNAL GLOBAL b  <REAL>
 ENTRY f EXTERNAL PARAM RNeg 0 <REAL>
 ENTRY g EXTERNAL RETURN RNeg <REAL>
 ENTRY i CONST INTERNAL GLOBAL c  <REAL>
 ENTRY k CONST INTERNAL GLOBAL d  <INT>
 ENTRY l INTERNAL GLOBAL result  <BOOL>
 ENTRY m EXTERNAL PARAM IntToReal 0 <INT>
 ENTRY n EXTERNAL RETURN IntToReal <REAL>
 ENTRY p EXTERNAL PARAM RMul 0 <REAL>
 ENTRY q EXTERNAL PARAM RMul 1 <REAL>
 ENTRY r EXTERNAL RETURN RMul <REAL>
 ENTRY u EXTERNAL PARAM RDivide 0 <REAL>
 ENTRY v EXTERNAL PARAM RDivide 1 <REAL>
 ENTRY w EXTERNAL RETURN RDivide <REAL>
 ENTRY y EXTERNAL PARAM RNotEqualTo 0 <REAL>
 ENTRY z EXTERNAL PARAM RNotEqualTo 1 <REAL>
 ENTRY A EXTERNAL RETURN RNotEqualTo <BOOL>
 ENTRY C EXTERNAL PARAM WriteBool 0 <BOOL>
DATA SECTION
 DEF a := 0 <INT>
 DEF GLOBAL b := a <INT>
 DEF c := 1.2 <REAL>
 DEF GLOBAL d := c <REAL>
 DEF e := 3.14 <REAL>
 CALL RNeg([e -> f]<REAL>) FROM DEF e := 3.14 <REAL>
 DEF h := (RETURN g) <REAL>
 DEF GLOBAL i := h <REAL>
 DEF j := 42 <INT>
 DEF GLOBAL k := j <INT>
BSS SECTION
 DEF GLOBAL l := FALSE <BOOL>
CODE SECTION
 CALL IntToReal([(GLOBAL b) -> m]<INT>) FROM CODE SECTION
 DEF o := (RETURN n) <REAL>
 CALL RMul([o -> p]<REAL>, [(GLOBAL d) -> q]<REAL>) FROM DEF o := (RETURN n) <REAL>
 DEF s := (RETURN r) <REAL>
 CALL IntToReal([(GLOBAL k) -> m]<INT>) FROM DEF s := (RETURN r) <REAL>
 DEF t := (RETURN n) <REAL>
 CALL RDivide([(GLOBAL i) -> u]<REAL>, [t -> v]<REAL>) FROM DEF t := (RETURN n) <REAL>
 DEF x := (RETURN w) <REAL>
 CALL RNotEqualTo([s -> y]<REAL>, [x -> z]<REAL>) FROM DEF x := (RETURN w) <REAL>
 DEF B := (RETURN A) <BOOL>
 GLOBAL l := B <BOOL>
 CALL WriteBool([(GLOBAL l) -> C]<BOOL>) FROM GLOBAL l := B <BOOL>
END
PROC SECTION
