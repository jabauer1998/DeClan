SYMBOL SECTION
 ENTRY b CONST INTERNAL GLOBAL a  <INT>
 ENTRY d CONST INTERNAL GLOBAL b  <REAL>
 ENTRY f EXTERNAL PARAM RNeg 0 <REAL>
 ENTRY g EXTERNAL RETURN RNeg <REAL>
 ENTRY i EXTERNAL PARAM IntToReal 0 <INT>
 ENTRY j EXTERNAL RETURN IntToReal <REAL>
 ENTRY l EXTERNAL PARAM RAdd 0 <REAL>
 ENTRY m EXTERNAL PARAM RAdd 1 <REAL>
 ENTRY n EXTERNAL RETURN RAdd <REAL>
 ENTRY p CONST INTERNAL GLOBAL c  <REAL>
 ENTRY q INTERNAL GLOBAL result  <REAL>
 ENTRY r INTERNAL GLOBAL bAndAResult  <REAL>
 ENTRY t EXTERNAL PARAM RMul 0 <REAL>
 ENTRY u EXTERNAL PARAM RMul 1 <REAL>
 ENTRY v EXTERNAL RETURN RMul <REAL>
 ENTRY x EXTERNAL PARAM RSub 0 <REAL>
 ENTRY y EXTERNAL PARAM RSub 1 <REAL>
 ENTRY z EXTERNAL RETURN RSub <REAL>
 ENTRY C EXTERNAL PARAM WriteString 0 <STRING>
 ENTRY D EXTERNAL PARAM WriteReal 0 <REAL>
DATA SECTION
 DEF a := 0 <INT>
 DEF GLOBAL b := a <INT>
 DEF c := 1.2 <REAL>
 DEF GLOBAL d := c <REAL>
 DEF e := 3.14 <REAL>
 CALL RNeg([e -> f]<REAL>)
 DEF h := (RETURN g) <REAL>
 CALL IntToReal([(GLOBAL b) -> i]<INT>)
 DEF k := (RETURN j) <REAL>
 CALL RAdd([h -> l]<REAL>, [k -> m]<REAL>)
 DEF o := (RETURN n) <REAL>
 DEF GLOBAL p := o <REAL>
BSS SECTION
 DEF GLOBAL q := 0.0 <REAL>
 DEF GLOBAL r := 0.0 <REAL>
CODE SECTION
 CALL IntToReal([(GLOBAL b) -> i]<INT>)
 DEF s := (RETURN j) <REAL>
 CALL RMul([(GLOBAL d) -> t]<REAL>, [s -> u]<REAL>)
 DEF w := (RETURN v) <REAL>
 GLOBAL r := w <REAL>
 CALL RSub([(GLOBAL p) -> x]<REAL>, [(GLOBAL r) -> y]<REAL>)
 DEF A := (RETURN z) <REAL>
 GLOBAL q := A <REAL>
 DEF B := "Result is " <STRING>
 CALL WriteString([B -> C]<STRING>)
 CALL WriteReal([(GLOBAL q) -> D]<REAL>)
 CALL WriteLn()
END
PROC SECTION
