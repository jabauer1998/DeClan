SYMBOL SECTION
 ENTRY b EXTERNAL PARAM RNeg 0 <REAL>
 ENTRY c EXTERNAL RETURN RNeg <REAL>
 ENTRY e CONST INTERNAL GLOBAL a  <REAL>
 ENTRY g CONST INTERNAL GLOBAL b  <REAL>
 ENTRY h INTERNAL GLOBAL result  <INT>
 ENTRY i EXTERNAL PARAM RAdd 0 <REAL>
 ENTRY j EXTERNAL PARAM RAdd 1 <REAL>
 ENTRY k EXTERNAL RETURN RAdd <REAL>
 ENTRY m EXTERNAL PARAM RealToInt 0 <REAL>
 ENTRY n EXTERNAL RETURN RealToInt <INT>
 ENTRY p EXTERNAL PARAM WriteReal 0 <INT>
DATA SECTION
 DEF a := 3.14 <REAL>
 CALL RNeg([a -> b]<REAL>)
 DEF d := (RETURN c) <REAL>
 DEF GLOBAL e := d <REAL>
 DEF f := 0.5 <REAL>
 DEF GLOBAL g := f <REAL>
BSS SECTION
 DEF GLOBAL h := 0 <INT>
CODE SECTION
 CALL RAdd([(GLOBAL e) -> i]<REAL>, [(GLOBAL g) -> j]<REAL>)
 DEF l := (RETURN k) <REAL>
 CALL RealToInt([l -> m]<REAL>)
 DEF o := (RETURN n) <INT>
 GLOBAL h := o <INT>
 CALL WriteReal([(GLOBAL h) -> p]<INT>)
END
PROC SECTION
