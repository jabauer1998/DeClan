SYMBOL SECTION
 ENTRY b CONST INTERNAL GLOBAL r1  <REAL>
 ENTRY d CONST INTERNAL GLOBAL r2  <REAL>
 ENTRY e INTERNAL GLOBAL result  <REAL>
 ENTRY f EXTERNAL PARAM RMul 0 <REAL>
 ENTRY g EXTERNAL PARAM RMul 1 <REAL>
 ENTRY h EXTERNAL RETURN RMul <REAL>
 ENTRY j EXTERNAL PARAM WriteReal 0 <REAL>
DATA SECTION
 DEF a := 21.0 <REAL>
 DEF GLOBAL b := a <REAL>
 DEF c := 2.0 <REAL>
 DEF GLOBAL d := c <REAL>
BSS SECTION
 DEF GLOBAL e := 0.0 <REAL>
CODE SECTION
 CALL RMul([(GLOBAL b) -> f]<REAL>, [(GLOBAL d) -> g]<REAL>) FROM CODE SECTION
 DEF i := (RETURN h) <REAL>
 GLOBAL e := i <REAL>
 CALL WriteReal([(GLOBAL e) -> j]<REAL>) FROM GLOBAL e := i <REAL>
END
PROC SECTION
