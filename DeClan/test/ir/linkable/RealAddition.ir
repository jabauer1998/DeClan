SYMBOL SECTION
 ENTRY a INTERNAL GLOBAL result  <REAL>
 ENTRY c CONST INTERNAL GLOBAL v1  <REAL>
 ENTRY e CONST INTERNAL GLOBAL v2  <REAL>
 ENTRY f EXTERNAL PARAM RAdd 0 <REAL>
 ENTRY g EXTERNAL PARAM RAdd 1 <REAL>
 ENTRY h EXTERNAL RETURN RAdd <REAL>
 ENTRY j EXTERNAL PARAM WriteReal 0 <REAL>
BSS SECTION
 DEF GLOBAL a := 0.0 <REAL>
DATA SECTION
 DEF b := 4.5 <REAL>
 DEF GLOBAL c := b <REAL>
 DEF d := 8.5 <REAL>
 DEF GLOBAL e := d <REAL>
CODE SECTION
 CALL RAdd([(GLOBAL c) -> f]<REAL>, [(GLOBAL e) -> g]<REAL>)
 DEF i := (RETURN h) <REAL>
 GLOBAL a := i <REAL>
 CALL WriteReal([(GLOBAL a) -> j]<REAL>)
END
PROC SECTION
