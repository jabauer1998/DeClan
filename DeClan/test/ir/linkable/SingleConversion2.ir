SYMBOL SECTION
 ENTRY b CONST INTERNAL GLOBAL r  <REAL>
 ENTRY c INTERNAL GLOBAL result  <INT>
 ENTRY d EXTERNAL PARAM RealToInt 0 <REAL>
 ENTRY e EXTERNAL RETURN RealToInt <INT>
 ENTRY g EXTERNAL PARAM WriteInt 0 <INT>
DATA SECTION
 DEF a := 6.5 <REAL>
 DEF GLOBAL b := a <REAL>
BSS SECTION
 DEF GLOBAL c := 0 <INT>
CODE SECTION
 CALL RealToInt([(GLOBAL b) -> d]<REAL>) FROM CODE SECTION
 DEF f := (RETURN e) <INT>
 GLOBAL c := f <INT>
 CALL WriteInt([(GLOBAL c) -> g]<INT>) FROM GLOBAL c := f <INT>
END
PROC SECTION
