 ENTRY f27 INTERNAL GLOBAL t  <INT>
 ENTRY f28 INTERNAL GLOBAL d  <INT>
 ENTRY f30 CONST INTERNAL GLOBAL b  <INT>
 ENTRY f32 CONST INTERNAL GLOBAL c  <INT>
 ENTRY f39 INTERNAL tempReg WriteInt <INT>
 ENTRY f40 INTERNAL PARAM i WriteInt 0 <INT>
BSS SECTION
 DEF GLOBAL f24 := 0 <INT>
 DEF GLOBAL f25 := 0 <INT>
 DEF GLOBAL f26 := 0 <INT>
 DEF GLOBAL f27 := 0 <INT>
 DEF GLOBAL f28 := 0 <INT>
DATA SECTION
 DEF GLOBAL b := 80 <INT>
 DEF GLOBAL c := 40 <INT>
 DEF GLOBAL f30 := 1 <INT>
 DEF GLOBAL f32 := 8 <INT>
CODE SECTION
 GLOBAL f26 := (GLOBAL b) <INT>
