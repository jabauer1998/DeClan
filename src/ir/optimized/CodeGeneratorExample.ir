PROC SECTION
 PROC LABEL WriteInt
  IPARAM f39 <INT> %dr
  IASM "MOV %dr, R0"
  IPARAM (PARAM f40) <INT> %ua
  IASM "LDR R0, %ua"
  IASM "SWI 1"
  IPARAM f39 <INT> %ur
  IASM "MOV R0, %ur"
 RETURN FROM WriteInt
