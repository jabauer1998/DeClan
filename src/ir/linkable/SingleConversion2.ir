SYMBOL SECTION
 ENTRY a INTERNAL GLOBAL result  <INT>
 ENTRY c CONST INTERNAL GLOBAL r  <REAL>
 ENTRY d EXTERNAL PARAM RealToInt 0 <REAL>
 ENTRY e EXTERNAL RETURN RealToInt <INT>
 ENTRY g EXTERNAL PARAM WriteInt 0 <INT>
BSS SECTION
 DEF GLOBAL a := 0 <INT>
DATA SECTION
 DEF b := 6.5 <REAL>
 DEF GLOBAL c := b <REAL>
CODE SECTION
 CALL RealToInt([(GLOBAL c) -> d]<REAL>)
 DEF f := (RETURN e) <INT>
 GLOBAL a := f <INT>
 CALL WriteInt([(GLOBAL a) -> g]<INT>)
END
PROC SECTION
